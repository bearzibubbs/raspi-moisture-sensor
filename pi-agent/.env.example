# Pi Agent Environment Configuration
# Copy this to .env and fill in actual values

# Orchestrator URL (required)
ORCHESTRATOR_URL=https://orchestrator.example.com

# Agent authentication token (obtain from bootstrap process)
# Leave empty for initial registration
AGENT_TOKEN=

# Logging level
LOG_LEVEL=INFO

# Override paths (optional)
# CONFIG_FILE=/app/config.yaml
# DB_PATH=/data/agent.db
